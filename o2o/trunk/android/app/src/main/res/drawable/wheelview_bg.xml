<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <gradient
                android:angle="90"
                android:centerColor="@color/gray_ddd"
                android:endColor="@color/black_light"
                android:startColor="@color/black_light" />

            <stroke
                android:width="1dp"
                android:color="@color/black" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="4dp"
        android:right="4dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <gradient
                android:angle="90"
                android:centerColor="@color/white"
                android:endColor="@color/gray_aaa"
                android:startColor="@color/gray_aaa" />
        </shape>
    </item>

</layer-list>